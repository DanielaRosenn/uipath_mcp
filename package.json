{
  "name": "@uipath/mcp-server",
  "version": "1.0.0",
  "description": "Production-ready Model Context Protocol (MCP) server for UiPath Orchestrator - enables AI assistants to interact with UiPath automation workflows, queues, jobs, and more",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "uipath-mcp-server": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "watch": "tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "inspect": "npx @modelcontextprotocol/inspector node dist/index.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "uipath",
    "orchestrator",
    "rpa",
    "automation",
    "ai",
    "claude",
    "llm",
    "workflow",
    "robotic-process-automation"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/uipath-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/uipath-mcp-server/issues"
  },
  "homepage": "https://github.com/yourusername/uipath-mcp-server#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^17.2.4",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  }
}
